---
//library imports
import { Icon } from 'astro-icon'

//data imports
import navData from '../data/navData'
---

<nav aria-label="Primary">
  <div class="navbar">
    <a href="/" aria-label="Go Home" class="logomark">
        <Icon name="logo" width="50"/>
    </a>
    <ul>
      {
        navData?.map(item=> <a href={item?.path}>{item?.name}</a>)
      }
    </ul>
  </div>
</nav>
